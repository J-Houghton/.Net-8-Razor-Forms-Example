<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FormApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src="~/js/site.js" asp-append-version="true"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/FormApp.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components.css" asp-append-version="true" />
</head>
<body>
    <header class="site-header">
        <div class="top-bar">
            <div class="container">
                <a class="university-name" href="https://www.test.domain">Some Name GoHere</a>
                <a class="directory-link" href="https://www.test.domain/directories/">Browse A-Z</a>
            </div>
        </div>

        <nav class="main-navigation">
            <div class="container">
                <div class="bar-logo"> </div>
                <div class="vertical-divider"></div>
                <div class="site-title">
                    <a href="https://www.test.domain/finsvcs/">This Is A Sub Name Here</a>
                </div>
            </div>
        </nav>

        <div class="color-bar"></div>
    </header>

    <div class="main-container">

        <main role="main" class="pb-3 main-content">
            <div class="breadcrumb-container">
                <ul class="breadcrumbs-list">
                    <li class="breadcrumbs-item">
                        <div class="home-svg-icon" style="margin-right: 0.5rem;">
                        </div>
                        <a class="breadcrumbs-link" href="/">home</a>
                    </li>
                    <li class="breadcrumbs-item"><a class="breadcrumbs-link" href="/">Administration</a></li>
                    <li class="breadcrumbs-item">
                        <a class="breadcrumbs-link" href="/">
                            Services
                        </a>
                    </li>
                    <li class="breadcrumbs-item">
                        <a class="breadcrumbs-link" href="/">
                            Cashiers
                        </a>
                    </li>
                    <li class="breadcrumbs-item">
                        <a class="breadcrumbs-link" href="/">
                            Pay Now
                        </a>
                    </li>
                </ul>
            </div>
            @RenderBody()
        </main>

    </div>
