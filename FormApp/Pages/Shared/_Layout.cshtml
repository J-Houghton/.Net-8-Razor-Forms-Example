    </svg>
    <footer class="global-footer">
        <div class="global-footer__container">
            <div class="global-footer__inner">
                <!-- Social Media Links -->
                <div class="social-links">
                    <a href="https://www.facebook.com/Test" aria-label="facebook" class="social-link">
                        <svg class="brei-icon brei-icon-facebook">
                            <use xlink:href="#brei-icon-facebook"></use>
                        </svg>
                    </a>
                    <a href="https://twitter.com/Test" aria-label="twitter" class="social-link left-mar">
                        <svg class="brei-icon brei-icon-twitter">
                            <use xlink:href="#brei-icon-twitter"></use>
                        </svg>
                    </a>
                    <a href="https://www.youtube.com/user/Test" aria-label="youtube" class="social-link left-mar">
                        <svg class="brei-icon brei-icon-youtube">
                            <use xlink:href="#brei-icon-youtube"></use>
                        </svg>
                    </a>
                    <a href="https://instagram.com/Test" aria-label="instagram" class="social-link left-mar">
                        <svg class="brei-icon brei-icon-instagram">
                            <use xlink:href="#brei-icon-instagram"></use>
                        </svg>
                    </a>
                </div>
                <!-- Address Information -->
                <div class="address-container">
                    <p><a href="https://www.test.edu/finsvcs/" class="global-footer__address-link">Systems</a></p>
                    <p>Tests</p>
                    <p> Test 2</p>
                    <p>123 ASD-ASDASD Dr. South, Someplace, ?? 12345</p>
                    </p>
                </div>
            </div>
        </div>

        <!-- Secondary Footer -->
        <div class="secondary-footer">
            <div class="secondary-footer__inner">
                <!-- Logo -->
                <a class="footer-cus__logo" href="https://www.test.domain">
                    <img src="" alt=" ">
                </a>


                <!-- Footer Menu -->
                <div class="footer-menu-container">
                    <ul class="footer-menu">
                        <li class="footer-menu-item"><a href="https://www.test.domain/contact/">Contact Us</a></li>
                        <li class="footer-menu-item"><a href="https://www.test.domain/advisories/">Emergency</a></li>
                        <li class="footer-menu-item"><a href="https://www.test.domain/disclosures/">Statements and Disclosures</a></li>
                        <li class="footer-menu-item"><a href="https://www.test.domain/about/values/accessibility/">Accessibility</a></li>
                        <li class="footer-menu-item"><a href="g"><img class="degree-stats" src="" alt=" Statistics"></a></li>
                        <li class="footer-menu-item"><a class="footer-cus__menu-link" href="https://www.test.domain/">Statement</a></li>
                    </ul>
                    <ul class="footer-menu">
                        <li class="footer-menu-item"><a href="https://www.test.domain/about/copyright.html">© </a>
                        <li class="footer-menu-item"><a style="color: #000;">Updated 1/1/1111 </a>
                    </ul>
                </div>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
